{
  "schemaVersion": 1,
  "id": "base-finder",
  "version": "${version}",
  "name": "Base Finder",
  "description": "A Meteor client addon for finding bases on anarchy servers.",
  "authors": [
    "omtoi"
  ],
  "contact": {
    "sources": "https://github.com/omtoi101/stash-finder"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.baseminer.basefinder.BaseFinder"
    ]
  },
  "mixins": [
    "addon-template.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}", "1.21.7", "1.21.6"],
    "meteor-client": "*",
    "fabric-networking-api-v1": "*",
    "fabric-api-base": "*"
  }
}
